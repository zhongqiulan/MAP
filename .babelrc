{
    "presets": [
        {
            "presets": [
                "es2015"
            ],
            "plugins": [
                "transform-vue-jsx"
            ]
        }
    ],
    "plugins": [
        [
            "import",
            {
                "libraryName": "iview",
                "libraryDirectory": "src/components"
            }
        ],
        [
            "component",
            {
                "libraryName": "element-ui",
                "styleLibraryName": "theme-chalk"
            }
        ],
        [
            "syntax-dynamic-import"
        ]
    ]
}